@{
    ViewData["Title"] = "Users";
}
@model List<RtfWebApp.Models.View.UserViewModel>

<script src="~/js/amcharts/amcharts.js"></script>
<link href="~/css/user.css" type="text/css" rel="stylesheet" />


<h2>@ViewData["Title"]</h2>


<div class="row">
    <div class="col-lg-12">
        @foreach (var user in Model)
        {
            <img src="#" alt="" class="profile profile-row profile-@user.AvatarId" />
            <a href="@Url.Action("User", "Home", new {id = user.Id})" target="_blank">@user.Name</a>
            <br />
        }
    </div>
</div>