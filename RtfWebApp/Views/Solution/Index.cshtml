@{
    ViewData["Title"] = "Solutions";
}
@model List<RtfWebApp.Models.View.SolutionViewModel>

<script src="~/js/amcharts/amcharts.js"></script>
<link href="~/css/user.css" type="text/css" rel="stylesheet" />


<h2>@ViewData["Title"]</h2>


<div class="row">
    <div class="col-lg-12">
        @foreach (var item in Model)
        {
            <a href="@Url.Action("Item", "Solution", new {id = item.Id})" target="_blank">@item.Name</a>
            <br />
        }
    </div>
</div>